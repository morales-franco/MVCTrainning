@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1 id="title-id" style="display:none">ASP.NET</h1>
    <p class="lead" id="description-id">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-4">
        <h1 id="async-id" >Async Test</h1>
        <p>
            @ViewBag.AsyncMessage
        </p>
    </div>
</div>

@section scripts{
    <script src="~/Scripts/App/home.js"></script>
}